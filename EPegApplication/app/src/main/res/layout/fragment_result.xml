<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:gravity="center"
    >

    <TextView
        android:id="@+id/end_study_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:layout_margin="@dimen/fab_margin"
        android:layout_toStartOf="@id/end_study_button"
        android:text="@string/study_end"
        android:textAlignment="center"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="@dimen/info_text_size" />

    <Button
        android:id="@+id/end_study_button"
        android:layout_width="525dp"
        android:layout_height="wrap_content"
        android:text="@string/end_study_button" />

    <TextView
        android:id="@+id/result_participant_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="TextView" />


</LinearLayout>